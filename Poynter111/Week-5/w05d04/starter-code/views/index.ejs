
<% for(type in messages){ %>
  <p><%= messages[type]  %></p>
  <% } %>
<% if(locals.isLoggedIn){ %>
  <h1>Welcome <%= locals.user.username %></h1>
  <a href='/logout'>Logout</a>

<% }else{%>
  <a href='/login'>Login</a>
  or
  <a href='/register'>Sign up</a>
<% } %>

<% users.forEach((user) => { %>
  <h1><%= user.username %></h1>
  <h2><%= user.email %></h2>
  <p><%= user.password %></p>
<% }) %>
